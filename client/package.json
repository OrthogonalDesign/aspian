{
  "name": "aspian-cms-react",
  "version": "1.0.0",
  "description": "ASPIAN CMS front-end with React.",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config=config/webpack.dev.js",
    "server": "dotnet watch -p ../API run",
